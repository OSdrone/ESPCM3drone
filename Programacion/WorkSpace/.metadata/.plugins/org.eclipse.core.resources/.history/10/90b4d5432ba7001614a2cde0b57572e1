#include "TASK_COMPASS.h"

void COMPASS_TASK_FCN(void const * argument){

	while(1){
		xSemaphoreTake(COMPASS_SMPHRHandle, portMAX_DELAY);
	}
}

void COMPASS_TIMER_FCN(void const * argument){
	xSemaphoreGive(COMPASS_SMPHRHandle);
}
