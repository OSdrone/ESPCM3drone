#include "TASK_IMU.h"


void IMU_TASK_FCN(void const * argument){

	while(1){
		xSemaphoreTake(IMU_SMPHRHandle, portMAX_DELAY);

		HAL_I2C_Master_Transmit_DMA(hi2c2, );
	}
}

void IMU_TIMER_FCN(void const * argument){
	xSemaphoreGive(IMU_SMPHRHandle);
}
