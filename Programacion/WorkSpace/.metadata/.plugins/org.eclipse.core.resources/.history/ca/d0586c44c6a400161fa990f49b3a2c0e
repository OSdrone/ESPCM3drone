#include "TASK_CONTROL.h"

void CONTROL_TASK_FCN(void const * argument){

	while(1){
		xSemaphoreTake(CONTROL_SMPHRHandle);

	}
}

void CONTROL_TIMER_FCN(void const * argument){
	xSemaphoreGive(CONTROL_SMPHRHandle);
}
