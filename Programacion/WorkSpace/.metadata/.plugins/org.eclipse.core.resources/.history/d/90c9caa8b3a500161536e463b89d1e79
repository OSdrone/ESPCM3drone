#ifndef OSVARIABLES_H_
#define OSVARIABLES_H_

typedef enum tpEstado{ESTADO_PARADA, ESTADO_ARRANCADO, ESTADO_VUELO, ESTADO_EMERGENCIA,
	ESTADO_IDENTIFICACION, ESTADO_CALIBRACION};
typedef enum tpModoTelemetria{CALIBRACION_IMU, CALIBRACION_BRUJULA};
typedef enum {NO_TELEMETRIA, TELEMETRIA_BASICA, TELEMETRIA_COMPLETA};
typedef enum {CONTROL_3EJES, CONTROL_EMPUJE, CONTROL_ALTURA, CONTROL_ACRO}tpModoControl;
typedef enum {MODO_IDENTIFICACION, MODO_IDENTIFICACION_VUELO}tpModoIdentificacion;


#endif /* OSVARIABLES_H_ */
