#ifndef OSVARIABLES_H_
#define OSVARIABLES_H_

typedef enum {ESTADO_PARADA, ESTADO_ARRANCADO, ESTADO_VUELO, ESTADO_EMERGENCIA}tpEstado;
typedef enum {NO_TELEMETRIA, TELEMETRIA_BASICA, TELEMETRIA_COMPLETA}tpModoTelemetria;
typedef enum {CONTROL_3EJES, CONTROL_EMPUJE, CONTROL_ALTURA, CONTROL_ACRO}tpModoControl;
typedef enum {MODO_VUELO, MODO_IDENTIFICACION, MODO_IDENTIFICACION_VUELO}tpModo;


#endif /* OSVARIABLES_H_ */
